{
  "expo": {
    "name": "GottaGo | Find Toilets Nearby",
    "slug": "gottago",
    "version": "1.1",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./assets/images/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.sigh10.gottago",
      "infoPlist": {
        "NSLocationWhenInUseUsageDescription": "This app uses your location to show nearby bathrooms."
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      }
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      "expo-secure-store"
    ],
    "experiments": {
      "tsconfigPaths": true
    },
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "bd9d3711-1863-4b63-8d1c-dd0e058b5699"
      }
    },
    "runtimeVersion": {
      "policy": "appVersion"
    },
    "updates": {
      "url": "https://u.expo.dev/bd9d3711-1863-4b63-8d1c-dd0e058b5699"
    }
  },
  "react-native-google-mobile-ads": {
    "ios_app_id": "ca-app-pub-7128214963424433~4184828336"
  }
}
